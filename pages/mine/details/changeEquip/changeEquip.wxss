.search {
    display: flex;
    justify-content: flex-start;
    background-color: #fafafa;
    padding: 20rpx;
    font-size: 32rpx;
    align-items: center,
}

.search image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.search text {
    color: #888888;
}

.main {
    border-bottom: 10rpx solid #eeeeee;
    padding: 20rpx;
}
.main .head {
    display: flex;
    justify-content: flex-end;
    align-items: center;
  
}

.main .equipName {
    display: flex;
    justify-content: center;
    font-size: 40rpx;
    margin: 10rpx; 
}

.main .details {
    display: flex;
    flex-direction: column;
    font-size: 32rpx;
    color: #959595;
    margin-left: 50rpx;
    margin-top: 50rpx;
}

.main .check {
    display: flex;
    justify-content: flex-end;
    margin: 20rpx 40rpx;
    color: #3E606F;
    font-size: 35rpx;
}
