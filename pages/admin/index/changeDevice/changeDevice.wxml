<view class="search">
    <image src="/img/mine/search.png"></image>
    <text>查找</text>
</view>
<view class="container1" wx:for="{{equipment}}"  wx:key="{{index}}">
    <view class="main">
        <view class="head">
            <text style="font-size: 35rpx; color:#1897b9">{{item.state}}</text>
        </view>
        <view class="equipName">{{item.id}}</view>
        <view class="equipName">{{item.name}}</view>
        <view class="details">
            <text>工作原理：{{item.workingPrinciple}}</text>
            <text>主要参数：{{item.mainParameter}}</text>
        </view>
        <view class="check">
            <text>查看具体信息</text>
        </view>
    </view>
</view>